<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unloadarr.unloadarr" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.unloadarr.unloadarr.startActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".menuActivity"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name="com.unloadarr.unloadarr.enterDateActivity"
            android:label="@string/title_activity_enter_date" >
        </activity>
        <activity
            android:name="com.unloadarr.unloadarr.viewDatesActivity"
            android:label="@string/title_activity_view_dates" >
        </activity>
        <activity
            android:name="com.unloadarr.unloadarr.checkDateActivity"
            android:label="@string/title_activity_check_date" >
        </activity>
        <activity
            android:name="com.unloadarr.unloadarr.aboutUsActivity"
            android:label="@string/title_activity_about_us" >
        </activity>
    </application>

</manifest>
